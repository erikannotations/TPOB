@article{haynes_falling_2020,
	title = {“{Falling} into the {Sky}”: {Gravity} and {Levity} in {Pynchon}’s \textit{{Mason} \& {Dixon}}},
	volume = {23},
	issn = {1206-3312, 1552-8308},
	shorttitle = {“{Falling} into the {Sky}”},
	url = {http://journals.sagepub.com/doi/10.1177/1206331219877141},
	doi = {10.1177/1206331219877141},
	abstract = {My argument follows geographer Gunnar Olsson when he asks, “What is geography if it is not the drawing and interpreting of a line? And what is the drawing of a line if it is not also the creation of new objects?” Using Thomas Pynchon’s 1997 novel Mason \& Dixon ( MD), about the drawing of the Mason-Dixon line, I explore how the mapmaker’s productive power is never merely reflective, but generative too, constructing a world as much as representing one. I question the consequent relation between “above and below,” drawing on Farinelli’s insight that critique of such constructions must recognize an antagonistic humor in the production of maps and territories. Pynchon’s novel, I argue, is exemplary in the wit with which it pits the anomalous, strange, and contingent phenomena of the below against the homogenizing, categorizing power of above. His approach helps us understand the dark heart of Enlightenment cartography and society.},
	language = {en},
	number = {4},
	urldate = {2024-06-28},
	journal = {Space and Culture},
	author = {Haynes, Doug},
	month = nov,
	year = {2020},
	pages = {357--369},
	file = {Accepted Version:/Users/erik/Zotero/storage/YAF8AR4K/Haynes - 2020 - “Falling into the Sky” Gravity and Levity in Pync.pdf:application/pdf},
}

@incollection{meiner_sous_2024,
	address = {New York, NY},
	series = {Routledge studies in comparative literature},
	title = {Sous la plage, les pavés! {The} beach between {Utopia} and {Dystopia} in {Thomas} {Pynchon}'s {Inherent} {Vice} and {Cormac} {McCarthy}'s {The} {Road}},
	isbn = {978-1-03-252673-7 978-1-03-252674-4},
	abstract = {This chapter discusses the beach in Inherent Vice from 2009 and The Road from 2006 in relation with the concepts of utopia and dystopia. While utopia for some critics constitutes a necessity and a persistent ideal, the chapter argues that, in the stories in question, the utopian vision is presented in a darker version that denies consummation of any utopia but instead turns it into a dystopia. The beach plays a significant role in this mutation as the beach is in constant relation to history, which becomes a central factor in the symbolic meaning of the beach. Despite the variations to this view, which arose in relation to the great wars when the beaches became a potential entry point for massive invasions, the beach has returned to an idea of positive connotations, and these are employed in the contemporary novels by Pynchon and McCarthy in order to activate our longing for a better world. Nevertheless, this longing is undermined, and utopia morphs into dystopia. The chapter argues that both novels show that it is possible to imagine utopian alternatives to the existing order and that the beach plays a vital role in the utopian outlay of the world even if it is unfolded in constant interaction with a neighborly dystopia.},
	booktitle = {The literary beach: history and aesthetics of a modern topos},
	publisher = {Routledge},
	author = {Andersen, Tore Rye},
	editor = {Meiner, Carsten and Andersen, Katrine Helene},
	year = {2024},
	keywords = {Literary criticism, Beaches in literature, Essays},
}

