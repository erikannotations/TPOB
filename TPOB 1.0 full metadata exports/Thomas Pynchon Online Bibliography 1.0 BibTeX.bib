@article{grmusa_cinematic_2017,
	title = {“{Cinematic}” {Gravity}’s {Rainbow}: {Indiscernibility} of the {Actual} and the {Virtual}},
	volume = {1},
	copyright = {http://creativecommons.org/licenses/by-nc-nd/4.0},
	issn = {2451-3474},
	shorttitle = {“{Cinematic}” {Gravity}’s {Rainbow}},
	url = {https://www.degruyter.com/document/doi/10.1515/culture-2017-0023/html},
	doi = {10.1515/culture-2017-0023},
	abstract = {Abstract
            Acknowledging that all media interpenetrate without any one being privileged as original, this paper focuses on the influence and omnipresence of screen technologies, cinematography in particular, asserting its capacities to act as a dominant technological and cultural force, which surveys and controls the economy, affects human consciousness, and implements its own ideologically tainted reality. More specifically, the analysis demonstrates the impact of cinematography within Thomas Pynchon’s Gravity’s Rainbow, emphasizing intermedial reflexivity from screen to paper, its unfolding and animation, representations and mutations, but also the notion that virtuality dominates actuality (Virilio, The Vision 63), or, in Deleuze’s terms, the two coalesce within the crystal-image, for the real and the imaginary, the actual and the virtual, although distinct, are indiscernible (Deleuze, Cinema 2 68-69). The study is grounded in Paul Virilio’s theoretical framework and his criticism of the colonization of the real by the virtual, and in Gilles Deleuze’s account on cinematic taxonomy and how cinematography partakes in the emergence of a new notion of reality and history. By appropriating, repurposing, and reshaping the techniques, forms, and contents of cinematography, and at the same time being critical of its effects, Pynchon uses paper surface for transition of his ideas, validating the repercussions of intermediation as a cultural force and unveiling literature’s performativity, its ability to shelter/entertain the “cinematic,” which in turn revolutionizes and animates fiction.},
	number = {1},
	urldate = {2024-03-29},
	journal = {Open Cultural Studies},
	author = {Grmusa, Lovorka Gruic},
	month = nov,
	year = {2017},
	pages = {257--269},
	file = {Full Text:/Users/erik/Zotero/storage/R4598JA9/Grmusa - 2017 - “Cinematic” Gravity’s Rainbow Indiscernibility of.pdf:application/pdf},
}

@article{guillen_pynchons_2000,
	title = {Pynchon’s {Social} {Aesthetics}: {Sixties} {Activism} in {PostModern} {American} {Literature}},
	volume = {84},
	issn = {0397-7870},
	shorttitle = {Pynchon’s {Social} {Aesthetics}},
	url = {https://www.persee.fr/doc/rfea_0397-7870_2000_num_84_1_1809},
	doi = {10.3406/rfea.2000.1809},
	abstract = {Dans la littérature américaine postmoderniste on trouve souvent non seulement un rejet des valeurs sociales de l '« establishment » mais aussi un rejet de la réalité elle-même. Gerald Graff, par exemple, a dénoncé la déconstruction comme une fuite devant les problèmes de la société moderne. Graff voit « la même influence débilitante s 'exercer dans le genre de fiction postmoderniste pratiqué par des écrivains comme Thomas Pynchon ». Si le réalisme est simplement un problème de convention dominante ne s 'attachant pas plus à la vérité que d'autres modes d'écriture plus artificiels, alors c 'est la porte ouverte, selon Graff, à « un scepticisme général que ne reconnaît que le jeu infini du texte». Thomas Pynchon, le représentant le plus éminent et le symbole le plus durable de cette époque semble effectivement travailler sur des thèmes qui font écho aux assertions postmodernistes sur l'inscrutabilité, la relativité et enfin de compte la subjectivité de la réalité. Cependant cet article défend l'idée qu'une étude précise de la production littéraire soigneusement travaillée de Pynchon montre qu 'il n 'en est rien.},
	language = {fr},
	number = {1},
	urldate = {2024-03-29},
	journal = {Revue française d’études américaines},
	author = {Guillen, Matthew},
	year = {2000},
	pages = {106--125},
}

